{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "Quantity": {
      "type": "number",
      "minimum": 1,
      "maximum": 10,
      "$entities": [
        "number"
      ]
    },
    "Length": {
      "$schema": "http://json-schema.org/draft-07/schema",
      "type": "object",
      "$entities": [
        "dimension"
      ],
      "required": [
        "number",
        "units"
      ],
      "properties": {
        "number": {
          "type": "number"
        },
        "units": {
          "type": "string"
        }
      }
    },
    "Name": {
      "type": "string",
      "$entities": [
        "personName",
        "utterance"
      ]
    },
    "Bread": {
      "type": "string",
      "enum": [
        "multiGrainWheat",
        "rye",
        "white",
        "wholeWheat"
      ],
      "$entities": [
        "BreadEntity"
      ]
    },
    "Meat": {
      "type": "string",
      "enum": [
        "bacon",
        "chicken",
        "ham",
        "pulled pork",
        "roast beef",
        "salami",
        "turkey",
        "none"
      ],
      "$entities": [
        "MeatEntity"
      ]
    },
    "Cheese": {
      "type": "string",
      "enum": [
        "american",
        "cheddar",
        "feta",
        "gouda",
        "pepper jack",
        "provolone",
        "swiss",
        "none"
      ],
      "$entities": [
        "CheeseEntity"
      ]
    },
    "Toppings": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "tomato",
          "lettuce",
          "pickles",
          "greenPeppers",
          "redPeppers",
          "whiteOnions",
          "redOnions"
        ],
        "maxItems": 3,
        "$entities": [
          "ToppingsEntity"
        ]
      },
    },
    "Sauces": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "pepper",
          "salt",
          "yellowMustard",
          "dijonMustard",
          "mayo",
          "vinegar"
        ]
      },
      "$entities": [
        "SaucesEntity"
      ]
    },
    "Price": {
      "$schema": "http://json-schema.org/draft-07/schema",
      "type": "object",
      "$entities": [
        "money"
      ],
      "required": [
        "number",
        "units"
      ],
      "properties": {
        "number": {
          "type": "number"
        },
        "units": {
          "type": "string"
        }
      }
    },
    "CancelConfirmation": {
      "type": "boolean",
      "$entities": [
        "boolean"
      ]
    },
    "CompleteConfirmation": {
      "type": "string",
      "$entities": [
        "boolean",
        "Quantity",
        "Length",
        "Name",
        "Bread",
        "Meat",
        "Cheese",
        "Toppings",
        "Sauces",
        "Price"
      ]
    },
    "PropertyToChange": {
      "type": "string",
      "$entities": [
        "Quantity",
        "Length",
        "Name",
        "Bread",
        "Meat",
        "Cheese",
        "Toppings",
        "Sauces",
        "Price"
      ]
    }
  },
  "required": [
    "Name",
    "Bread",
    "Cheese",
    "Meat",
    "Toppings",
    "Sauces",
    "Price"
  ],
  "$expectedOnly": [
    "boolean",
    "utterance",
    "keyPhrase",
    "personName"
  ],
  "$operations": [
    "Add()",
    "Remove()",
    "Clear()",
    "Show()",
    "Help()",
    "Change()"
  ],
  "$requiresValue": [
    "Add()",
    "Remove()"
  ],
  "$defaultOperation": {
    "CancelConfirmation": {
      "": "Set()"
    },
    "CompleteConfirmation": {
      "": "Set()"
    },
    "": {
      "": "Add()"
    }
  },
  "$parameters": [],
  "$public": [
    "Quantity",
    "Length",
    "Name",
    "Bread",
    "Meat",
    "Cheese",
    "Toppings",
    "Sauces",
    "Price"
  ],
  "definitions": {}
}