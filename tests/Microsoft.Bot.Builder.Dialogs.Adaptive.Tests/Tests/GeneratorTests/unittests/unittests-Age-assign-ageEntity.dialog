{
    "$schema": "../../../../tests.schema",
    "$kind": "Microsoft.OnAssignEntity",
    "property": "Age",
    "entity": "age",
    "actions": [
        {
            "$kind": "Microsoft.SendActivity",
            "activity": "${setObjectPropertyMessage('Age', concat(string(@age.number), ' ', @age.units))}"
        },
        {
            "$kind": "Microsoft.SetProperty",
            "property": "$Age.number",
            "value": "=@age.number"
        },
        {
            "$kind": "Microsoft.SetProperty",
            "property": "$Age.units",
            "value": "=@age.units"
        }
    ]
}