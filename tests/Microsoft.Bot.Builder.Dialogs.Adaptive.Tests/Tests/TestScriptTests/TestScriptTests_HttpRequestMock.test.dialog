{
    "$schema": "../../../tests.schema",
    "$kind": "Microsoft.Test.Script",
    "description": "Test HttpRequestMock",
    "httpRequestMocks": [
        "httpexact.mock",
        "httpgzip.mock",
        "httppartial.mock",
        "get.mock",
        "all.mock"
    ],
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "triggers": [
            {
                "$kind": "Microsoft.OnBeginDialog",
                "actions": [
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statusCode} ${dialog.result.reasonPhrase}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statusCode} ${dialog.result.reasonPhrase}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "GET",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "POST",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content.data}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "POST",
                        "url": "http://127.0.0.2",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.statuscode} ${dialog.result.reasonphrase}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "PATCH",
                        "url": "http://127.0.0.1",
                        "body": "full body or partial body",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "PATCH",
                        "body": "full body",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    },
                    {
                        "$kind": "Microsoft.HttpRequest",
                        "responseType": "json",
                        "method": "POST",
                        "body": "gzip body",
                        "url": "http://127.0.0.1",
                        "resultProperty": "dialog.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${dialog.result.content}"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserSays",
            "text": "hello"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "string response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "object response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "byte array response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "404 not found temporarily"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "500 internal server error temporarily"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "fallback response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated ByteArray response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "repeated ByteArray response"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "404 No matching mock handler for \"POST http://127.0.0.2/\""
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "partial match"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "exact match"
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "string response with gzip format"
        }
    ]
}
