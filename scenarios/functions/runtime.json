{
  "adapter":
  {
    "$kind": "Microsoft.BotCoreAdapter",
    "middleware": [
    ]
  },
  "channel": {
    "$kind": "Microsoft.DeclarativeChannelProvider",
    "channel": null
  },
  "credentials": {
    "$kind": "Microsoft.DeclarativeCredentialsProvider",
    "applicationId": "=MicrosoftAppId",
    "applicationPassword": "=MicrosoftAppPassword"
  },
  "defaultLocale": "en-US",
  "rootDialog": "=defaultRootDialog",
  "storage": {
    "$kind": "Microsoft.MemoryStorage"
  },
  "telemetry": {
    "$kind": "Microsoft.ApplicationInsightsTelemetryProvider",
    "instrumentationKey": "=applicationInsights.InstrumentationKey"
  }
}